<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="nyc.c4q.jonathancolon.inContaq.contactlist.fragments.GraphFragment">

    <android.support.v7.widget.CardView
        android:id="@+id/graph_cardview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/card_margin_end"
        android:layout_marginStart="@dimen/card_margin_start"
        android:layout_marginTop="@dimen/card_chart_margin_top"
        android:elevation="@dimen/card_elevation"
        android:padding="@dimen/card_chart_padding"
        app:cardBackgroundColor="#052635"
        app:cardCornerRadius="@dimen/card_chat_corner_radius">

        <com.db.chart.view.LineChartView
            android:id="@+id/daily_chart"
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_chart_height"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_margin="@dimen/line_chart_margin">
        </com.db.chart.view.LineChartView>
    </android.support.v7.widget.CardView>

</FrameLayout>
